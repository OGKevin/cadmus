{
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^\\.github\\/workflows\\/mdbook-pr\\.yml$/",
        "/^\\.github\\/workflows\\/mdbook-pages\\.yml$/"
      ],
      "matchStrings": ["MDBOOK_VERSION:\\s+\\\"(?<currentValue>[^\\\"]+)\\\""],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "rust-lang/mdBook",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^\\.github\\/workflows\\/mdbook-pr\\.yml$/",
        "/^\\.github\\/workflows\\/mdbook-pages\\.yml$/"
      ],
      "matchStrings": [
        "MDBOOK_EPUB_REV:\\s+\\\"(?<currentValue>[a-f0-9]{40})\\\""
      ],
      "datasourceTemplate": "git-refs",
      "depNameTemplate": "Michael-F-Bryan/mdbook-epub",
      "packageNameTemplate": "https://github.com/Michael-F-Bryan/mdbook-epub",
      "versioningTemplate": "git"
    },
    {
      "customType": "regex",
      "managerFilePatterns": ["/^devenv\\.nix$/"],
      "matchStrings": ["rev = \\\"(?<currentValue>[a-f0-9]{40})\\\""],
      "datasourceTemplate": "git-refs",
      "depNameTemplate": "Michael-F-Bryan/mdbook-epub",
      "packageNameTemplate": "https://github.com/Michael-F-Bryan/mdbook-epub",
      "versioningTemplate": "git"
    }
  ]
}
